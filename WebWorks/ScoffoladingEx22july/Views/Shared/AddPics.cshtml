@{
    List<String> pics = new List<string>
{
        "~/Content.cam.jpeg",
        "~/Content.photo.jpeg",
        "~/Content.Tig.jpeg",
        "~/Content.tiger.jpeg"
    };


<div>
    @foreach (var item in pics)
    {
        <img src="@Url.Content(item)" width="100" height="100" />
    }
</div>
}
